<h1>Create New Trader</h1>

<%= form_with model: @trader, url: admin_traders_path, method: :post do |form| %>
  <div>
    <%= form.label :email %><br>
    <%= form.email_field :email %>
  </div>

  <div>
    <%= form.label :password %><br>
    <%= form.password_field :password %>
  </div>

  <div>
    <%= form.label :password_confirmation %><br>
    <%= form.password_field :password_confirmation %>
  </div>

  <div>
    <%= form.submit "Create Trader" %>
  </div>
<% end %>
<%= link_to "Back to Traders List", admin_traders_path %>
