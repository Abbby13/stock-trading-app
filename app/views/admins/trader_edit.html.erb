<h1>Edit Trader</h1>

<%= form_with model: @trader, url: admin_trader_path(@trader), method: :patch do |form| %>
  <div>
    <%= form.label :email %><br>
    <%= form.email_field :email %>
  </div>

  <div>
    <%= form.label :password %><br>
    <%= form.password_field :password %>
  </div>

  <div>
    <%= form.label :password_confirmation %><br>
    <%= form.password_field :password_confirmation %>
  </div>

  <div>
    <%= form.submit "Update Trader" %>
  </div>
<% end %>
<%= link_to "Back to Trader", admin_trader_path(@trader) %>
